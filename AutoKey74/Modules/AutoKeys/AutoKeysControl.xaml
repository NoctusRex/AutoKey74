<UserControl x:Class="AutoKey74.Modules.AutoKeys.AutoKeysControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutoKey74.Modules.AutoKeys"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" UseLayoutRounding="True">
    <Grid Height="1000">
        <Label Content="Auto Keys" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" MinHeight="2" FontSize="36" Width="171"/>
        <ScrollViewer x:Name="ScrollViewerAutoKeys" Margin="10,73,10,10" UseLayoutRounding="False" Grid.IsSharedSizeScope="True" VerticalScrollBarVisibility="Auto">
            <StackPanel x:Name="StackPanelAutoKeys" CanHorizontallyScroll="True" CanVerticallyScroll="True"></StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
