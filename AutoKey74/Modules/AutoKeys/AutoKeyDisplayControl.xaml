<UserControl x:Class="AutoKey74.Modules.AutoKeys.AutoKeyDisplayControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutoKey74.Modules.AutoKeys"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="800" Height="50">
    <Grid Margin="0,2" Height="46">
        <CheckBox x:Name="CheckBoxEnabled" HorizontalAlignment="Left" Margin="8,10,0,0" VerticalAlignment="Top" FontSize="16" Click="CheckBoxEnabled_Click">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <Label x:Name="LabelInfo" Content="Minecraft Dungeons - 100ms - 100000ms - A, D" Margin="44,6,86,0" VerticalAlignment="Top" Height="34" Padding="0" FontSize="24"/>
        <Button x:Name="ButtonDelete" Content="X" HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top" Width="30" Height="30" Click="ButtonDelete_Click"/>
        <Button x:Name="ButtonEdit" Content="E" HorizontalAlignment="Right" Margin="0,10,51,0" VerticalAlignment="Top" Width="30" Height="30" Click="ButtonEdit_Click"/>
    </Grid>
</UserControl>
